  "rules": [
    {
      "id": "second_opinion",
      "topic": "Second opinion",
      "depts": ["front_desk", "phones", "triage"],
      "logic": {
        "constraints": [
          {"type": "must_be_physician_visit", "value": true},
          {"type": "no_double_book_adjacent_to_cancer_consult", "value": true}
        ]
      },
      "routing": {"allowed_roles": ["MD"], "disallowed_roles": ["APP"]},
      "notes": "Only schedule on physician. Do not double book before/after cancer consults."
    },
    {
      "id": "ed_new_patient",
      "topic": "Erectile Dysfunction (new patient)",
      "depts": ["front_desk", "phones", "triage"],
      "routing": {
        "allowed_providers": ["LAW","TPB","TMK","LWM"],
        "disallowed_providers": ["DLH","AJL","AJP","JRH","DBW"]
      },
      "notes": "NP-ED with LAW/TPB/TMK/LWM; do not schedule ED-NP with DLH/AJL/AJP/JRH/DBW."
    },
    {
